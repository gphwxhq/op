QPushButton {
    background-color: rgb(75, 75, 75);
    border-radius: 8px;
    padding: 16px 32px;
    text-align: center;
    text-decoration: none;
    font-size: 16px;
    margin: 4px 2px;
    color: white;
}

QPushButton:hover {
    background-color: white;
    border: 1px solid rgb(75, 75, 75);
    color: black
}

QPushButton:pressed {
    background-color: #06AD56;
}

QLineEdit{
    border-color: #333333;
    border: 1px solid #333333;
}
